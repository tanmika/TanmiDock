{
  "name": "tanmi-dock",
  "version": "0.1.0",
  "description": "集中型第三方库链接管理工具",
  "type": "module",
  "bin": {
    "tanmi-dock": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "symlink",
    "3rdparty",
    "dependency",
    "storage"
  ],
  "author": "tanmika",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/proper-lockfile": "^4.1.4",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.17.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
