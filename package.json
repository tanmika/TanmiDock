{
  "name": "tanmi-dock",
  "version": "0.5.0-beta.1",
  "description": "集中型第三方库链接管理工具",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "tanmi-dock": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanmika/TanmiDock.git"
  },
  "homepage": "https://github.com/tanmika/TanmiDock#readme",
  "bugs": {
    "url": "https://github.com/tanmika/TanmiDock/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/",
    "format:check": "prettier --check src/ tests/",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "symlink",
    "3rdparty",
    "dependency",
    "storage"
  ],
  "author": "tanmika",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "commander": "^12.1.0",
    "p-limit": "^7.2.0",
    "proper-lockfile": "^4.1.2",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
